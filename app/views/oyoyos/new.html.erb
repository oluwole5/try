<h1>New oyoyo</h1>

<% form_for(@oyoyo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= submit_tag 'yeap' %>
    <%= f.submit'submit' %>
  </p>
  <%end%>




<%= link_to 'Back', oyoyos_path %>